{
    "name": "prokki/htpasswd",
    "type": "library",
    "description": "This symfony user provider reads user from the htpasswd file.",
    "homepage": "https://github.com/prokki/htpasswd",
    "keywords": [
        "htpasswd",
        "basic",
        "authentication",
        "md5",
        "crypt",
        "bcrypt",
        "blowfish",
        "apache",
        "password"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Falko Matthies",
            "email": "falko.m@web.de"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "symfony/symfony": "^4.3",
        "whitehat101/apr1-md5": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Htpasswd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Htpasswd\\Tests\\": "tests/"
        }
    }
}
